        cursor: pointer;
        transition: 0.5s ease;
        }
        .left button:hover{
            background-color: #2e4450;
            color: white;
        }
        .section button{
          background-color: white;
          border: 0.01rem solid;
          border-radius: 2rem;
          width: 4rem;
          padding: 0.25rem;
          cursor: pointer;
          transition: 0.5s ease;
          color: black;
        }
        .section button:hover{
            background-color: #2e4450;
            color: white;
        }


        .section{
            margin-top: 14rem;
            display: flex;
            flex-direction: row;
            flex-wrap: wrap;
            justify-content: space-evenly;
            /* margin-bottom: 5rem; */
        }
        .section-head{
            display: flex;
            justify-content: center;
            margin-bottom: 5rem;
        }
        .override .card{
            box-shadow: 0px 4px 12px rgba(0, 0, 0, 0.5);
            border:none;
            /* max-width:23rem; */

        }
        #img{
            
            height: 10rem;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            margin-left: 2rem;
            margin-right: 2rem;
        }
        .card{
            width: 18rem;
            min-width: 20rem;
            box-shadow: 0px 8px 24px rgba(0, 0, 0, 0.1);
        }
        .card-header
        {
            text-align: center;
        }
        .card-body{
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            height:20rem;
        }
        .override .btn{
            margin-top: 6px;
        }
        .left button a{
            text-decoration: none;
            color:black;
        }
        .section .card:hover{
          transition: 0.5s ease-out;
          transform: translateY(-1rem);
        }
        @media screen and (max-width:768px) {
            .container{
                flex-direction: column;
                text-align: center;
                justify-content: center;
                align-items: center;
                margin-top: 1rem;
            }
            .container .left p, h1{
                margin: 2rem;
                width: 18rem;
            }
        }

        span{
          animation: blinker 1s linear infinite;
        }
        @keyframes blinker{
          50%{
