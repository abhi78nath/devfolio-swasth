      <script>
        var msgArr = ["Welcome"];
        var textPosition = 0;
        var speed = 300;

        typewriter = ()=>{
          document.querySelector("#msg").innerHTML = msgArr[0].substring(0, textPosition) + "<span>\u007c</span>";
          if(textPosition++ != msgArr[0].length)
          setTimeout(typewriter, speed);
        }
        window.addEventListener("load", typewriter);

        var loader = document.getElementById("preloader");
        window.addEventListener("load", function(){
          loader.style.display = "none";
        })
      </script>
      
