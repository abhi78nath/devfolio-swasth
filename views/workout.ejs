        #override .navbar{
            justify-content: center;
        }
        #override .head{
            margin-top: 2rem;
            text-align: center;
        }





        .card{
            width: 18rem;
            min-width: 20rem;
        }
        .card-header
        {
            text-align: center;
        }
        .card-body{
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            height:20rem;
        }
        .override .btn{
            margin-top: 6px;
        }
        .left button a{
            text-decoration: none;
            color:black;
        }

        .long-card{
            display: flex;
            max-width: 82rem;
            /* border: 2px solid; */
            box-shadow: 0px 4px 12px rgb(0 0 0 / 50%);
            margin-top: 15px;
            padding: 1rem;
        }
        .long-card .right{
