        transition: 0.5s ease;
        }
        button:hover{
            background-color: white;
        }



        .section{
            
            display: flex;
            flex-direction: column;
            flex-wrap: wrap;
            justify-content: space-evenly;
            margin-top: 3rem;
            margin-bottom: 5rem;
