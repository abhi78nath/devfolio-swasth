            /* justify-content: center; */
            flex-direction: column;
            align-items: center;
        }
        @media screen and (max-width:768px) {
            .container{
                flex-direction: column;
                text-align: center;
                justify-content: center;
                align-items: center;
                margin-top: 1rem;
            }
            .container .left p, h1{
                margin: 2rem;
                width: 18rem;
            }
            .long-card{
                display: flex;
                flex-direction: column;
                justify-content: center;
                align-items: center;
            }
            .text{
                margin-left: 2rem;
            }
        }
        @media screen and (max-width:322px){
            #img{
                width:19rem;
            }
            .long-card{
                padding: 0;
            }
        }
